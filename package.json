{
    "name": "RNStorybook",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "yarn run prestorybook && react-native run-android",
        "ios": "yarn run prestorybook && react-native run-ios",
        "start": "react-native start",
        "test": "jest",
        "lint": "eslint .",
        "prestorybook": "rnstl",
        "storybook": "start-storybook -p 7007",
        "build-react": "webpack --mode production",
        "start-react": "webpack-dev-server --config ./webpack.config.js --mode development",
        "start-storybook-web": "./node_modules/@storybook/react/bin/index.js",
        "build-storybook-web": "./node_modules/@storybook/react/bin/build.js",
        "storybook-web": "yarn run start-storybook-web",
        "build:storybook-web": "yarn run build-storybook-web",
        "web": "./node_modules/.bin/webpack-dev-server -inline"
    },
    "config": {
        "react-native-storybook-loader": {
            "searchDir": [
                "./src/components"
            ],
            "pattern": "**/*.stories.js",
            "outputFile": "./storybook/storyLoader.js"
        }
    },
    "dependencies": {
        "@babel/preset-typescript": "^7.15.0",
        "@eva-design/eva": "^2.1.1",
        "@storybook/addon-essentials": "^6.3.8",
        "@ui-kitten/components": "^5.1.1",
        "@ui-kitten/eva-icons": "^5.1.1",
        "hermes-engine": "^0.9.0",
        "moti": "^0.14.2",
        "react": "16.13.1",
        "react-native": "^0.65.1",
        "react-native-gesture-handler": "^1.10.3",
        "react-native-reanimated": "^2.0.0",
        "react-native-safe-area-context": "^3.3.2",
        "react-native-svg": "^12.1.1",
        "react-native-svg-web": "^1.0.9",
        "react-native-web": "^0.13.4",
        "styled-components": "^5.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@babel/plugin-proposal-decorators": "^7.15.4",
        "@babel/preset-env": "^7.10.4",
        "@babel/preset-react": "^7.10.4",
        "@babel/runtime": "^7.8.4",
        "@react-native-community/eslint-config": "^2.0.0",
        "@storybook/addon-actions": "^5.3.19",
        "@storybook/addon-links": "^5.3.19",
        "@storybook/addons": "^5.3.19",
        "@storybook/react": "^6.3.8",
        "@storybook/react-native": "^5.3.25",
        "@storybook/react-native-server": "^5.3.23",
        "@types/jest": "^27.0.1",
        "@types/react": "^17.0.20",
        "@types/react-native": "^0.65.0",
        "@types/react-test-renderer": "^17.0.1",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^25.1.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "eslint": "^7.5.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-react": "^7.20.3",
        "eslint-plugin-react-hooks": "^4.0.8",
        "eslint-plugin-react-native": "^3.8.1",
        "html-webpack-plugin": "^4.3.0",
        "jest": "^25.1.0",
        "metro-react-native-babel-preset": "^0.59.0",
        "prettier": "^2.0.5",
        "react-dom": "^16.13.1",
        "react-native-storybook-loader": "^2.0.2",
        "react-test-renderer": "16.13.1",
        "typescript": "^4.4.3",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0"
    },
    "jest": {
        "preset": "react-native"
    }
}
